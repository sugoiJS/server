{
  "name": "@sugoi/server",
  "version": "1.2.3-b1",
  "description": "sugoi framework server module, relays on express and inversify",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "..\\node_modules\\.bin\\typedoc --out ..\\dist\\docs\\server --json dist\\docs\\main.json --ignoreCompilerErrors --excludeNotExported --excludeProtected --excludePrivate --readme .\\README.md --mode file --hideGenerator --gitRevision '1.0.0' --externalPattern node_modules/**/* --excludeExternals",
    "update:patch": " npm run build && npm version patch && npm publish && npm run docs",
    "update:minor": " npm run build && npm version minor && npm publish && npm run docs",
    "update:major": " npm run build && npm version major && npm publish && npm run docs",
    "build": "rmdir /S dist && tsc"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "sugoi",
    "@sugoi",
    "server",
    "inversify",
    "decorator",
    "annotation",
    "express"
  ],
  "author": "Orel Balilti",
  "license": "MIT",
  "dependencies": {
    "@sugoi/core": "^1.2.0",
    "express": "^4.16.3",
    "inversify-express-utils": "^6.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "typedoc": "^0.11.1"
  }
}
